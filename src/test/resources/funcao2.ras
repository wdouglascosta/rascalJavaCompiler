program funcaoSimples;
  var x, k: integer;

  function fun1(a: integer, b: integer): integer;
  var soma: integer;
  begin
    soma := a + b;
  end

  procedure proc1(n: integer, g: integer);
    var h:integer;
  begin
    h := n + g;
    write(h);
  end

  function fun2(r: integer, s: integer): integer;
  var t :integer;
  begin
    t := t div s;
    fun1(r, t);
  end

  procedure proc2(n: integer);
    var w, g:integer;
  begin
    w := n * g;
    write(w);
  end

begin
  x := fun1(5, 10);
  write(x);
  k := 0;
  fun2(1, x);
  proc1(3, k);
  proc2(5);
end.